<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.staceylm.foodnutritionactivity.StartActivity">

    <Button
        android:id="@+id/bActivityTracker"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/bFoodTracker"
        android:layout_alignStart="@+id/bFoodTracker"
        android:layout_marginBottom="39dp"
        android:text="Activity Tracker"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="162dp" />

    <Button
        android:id="@+id/bFoodTracker"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/bThermostat"
        android:layout_alignStart="@+id/bThermostat"
        android:layout_marginBottom="53dp"
        android:text="Food Tracker"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="239dp" />

    <Button
        android:id="@+id/bThermostat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/bAutomobile"
        android:layout_alignStart="@+id/bAutomobile"
        android:layout_marginBottom="62dp"
        android:text="House Thermostat"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="303dp" />

    <Button
        android:id="@+id/bAutomobile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="110dp"
        android:layout_marginStart="105dp"
        android:text="Automobile"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="377dp" />
</RelativeLayout>
